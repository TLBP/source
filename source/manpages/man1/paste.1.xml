<refentry xmlns="http://docbook.org/ns/docbook" xml:id="tr-man1-paste" version="5.0" xml:lang="tr">
 <refmeta>
  <manvolnum>1</manvolnum>
  <refentrytitle>paste</refentrytitle>
  <refmiscinfo class="other" otherclass="header">Kullanıcı Komutları</refmiscinfo>
  <refmiscinfo class="other" otherclass="domain">coreutils 6.5</refmiscinfo>
