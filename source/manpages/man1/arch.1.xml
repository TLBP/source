 <refnamediv>
  <refname>arch</refname>
  <refpurpose>makine donanımının ismini basar (uname -m ile aynı)</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <cmdsynopsis>
   <command>arch</command>
   <arg rep='repeat'><replaceable>SEÇENEK</replaceable></arg>
  </cmdsynopsis>
 </refsynopsisdiv>
 <refsect1><title>AÇIKLAMA</title>
  <para>
   Makine mimarisini (örneğin, x86_64) basar.
  </para>
  <variablelist>
   <varlistentry>
    <term><option>--help</option></term>
    <listitem>
     <para>Bu yardım metnini basar ve çıkar.
    </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><option>--version</option></term>
    <listitem>
     <para>Sürüm bilgilerini basar ve çıkar.
    </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>
