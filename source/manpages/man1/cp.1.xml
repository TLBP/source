 xmlns:xlink="http://www.w3.org/1999/xlink" version="5.0" xml:lang="tr">
 <info>
  <keywordset>
   <keyword>Linux, manpage, kütüphane, kitaplık, belge, belgeler</keyword>
   <keyword>kabuk, komut, kullanıcı, yardım, kılavuz, coreutils</keyword>
