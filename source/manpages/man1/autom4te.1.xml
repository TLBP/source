<refentry xml:id="tr-man1-autom4te" xmlns="http://docbook.org/ns/docbook"
 xmlns:xlink="http://www.w3.org/1999/xlink" version="5.0" xml:lang="tr">
 <refmeta>
  <refentrytitle>autom4te</refentrytitle>
  <manvolnum>1</manvolnum>
  <refmiscinfo otherclass="header">Kullanıcı Komutları</refmiscinfo>
  <refmiscinfo otherclass="domain">GNU autoconf 2.71</refmiscinfo>
